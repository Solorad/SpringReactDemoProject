    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "0.14.5",
    "file-loader": "^0.8.5",
    "style-loader": "^0.13.0",
    "axios": "^0.15.3"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "babel-runtime": "^6.6.1",
