{
  "name": "spring-react-demo",
  "version": "1.0.0",
  "description": "Demo application with Spring Boot and ReactJS",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solorad/SpringReactDemoProject.git"
  },
  "keywords": [
    "morenkov",
    "springBoot",
    "ReactJs"
  ],
  "author": "Evgenii Morenkov",
