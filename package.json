  "bugs": {
    "url": "https://github.com/Solorad/SpringReactDemoProject/issues"
  },
  "homepage": "https://github.com/Solorad/SpringReactDemoProject#readme",
  "scripts": {
    "watch": "webpack --watch -d",
    "eslint": "eslint .; exit 0",
    "eslint-components": "eslint src/main/resources/static; exit 0",
    "build": "mvn -q clean compile && npm install && webpack",
    "serve": "webpack && mvn -q clean spring-boot:run",
